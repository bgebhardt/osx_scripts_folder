					<string># encoding: utf-8

if RUBY_VERSION.to_f &gt; 1.9
	Encoding.default_external = Encoding::UTF_8
	Encoding.default_internal = Encoding::UTF_8
end

pairs = [
	['"','"'],
	['\[','\]'],
	['\(','\)'],
	['\'','\''],
	['\{','\}']
]

ARGF.each {|line|
	links = line.scan(/(href="|\]\(|&lt;|\]: )?([\(\["\{]+)?((https?|ftp):\/\/[^\s]+)/)
	links.each {|match|
		if match[0].nil?
			link = match[2]
			pairs.each {|pair|
				if match[1] =~ /#{pair[0]}/ &amp;&amp; link =~ /#{pair[1]}/
					link = link.sub(/#{pair[1]}(?!.*#{pair[1]})$/,'')
				end
				link.sub!(/#{pair[1]}.{1,3}?$/,'')
			} unless match[1].nil?
			linkre = link.sub(/[!,\.\?;:]$/,'').gsub(/([\?\!\*\$\[\]\(\)])/,"\\\\\\1")
			line.sub!(/(#{linkre})/m,"&lt;\\1&gt;")
		end
	}
	puts line
}</string>
